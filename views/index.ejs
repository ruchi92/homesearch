<!DOCTYPE html>
<html>
  <head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Home Search Made Easy.!</title>
    <% include inclusions.ejs %>
    <link rel="stylesheet" href="/stylesheets/city_card.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--Range Sliders-->
    <script>
      function myFunction() {
        var x = document.getElementById("myRange").value;
        document.getElementById("demo").innerHTML = x * 114;
        document.getElementById("demo1").innerHTML = x * 125;
        document.getElementById("demo2").innerHTML = x * 12500;
      }
    </script>
    <style>
        .navbar {
            z-index: 100;
        }
    </style>
  </head>

  <body  id="page-top" onload = "header_change()">
  <% include header.ejs %>
    <!--Header Starts-->
    <header>
      <div class="header-content">
        <div class="header-content-inner">
          <!--Put the search blocks similiar to that of edesia-->

          <div class="row">
            <br><br><br>
            <table class="table">
              <thead style="margin-top: 20px"><b><i>Look for Homes in the Following Areas</i></b></thead>
              <tr class="row" style="margin: 10px; padding: 10px">
                <td class = "background" height="300" width="390" background="/images/places/la.jpg">
                  <p align="center" ><b><i>Los Angeles</i></b></p>
                  <ul>
                    <li><a href="#">Beverly Hills</a></li>
                    <li><a href="#">Long Beach</a></li>
                    <li><a href="#">Santa Monica</a></li>
                    <li><a href="#">Culver City</a></li>
                    <li><a href="#">Downtown</a></li>
                    <li><a href="#">Venice</a></li>
                    <li><a href="#">Marina Del Rey</a></li>
                    <li><a href="#">Manhattan Beach</a></li>
                  </ul>
                </td>
                &nbsp;
                <td  class = "background" height="300" width="390" background="/images/places/sf.jpg">
                  <p align = "center"><b><i>San Francisco</i></b></p>
                  <ul>
                    <li><a href="#">The Castro</a></li>
                    <li><a href="#">Downtown</a></li>
                    <li><a href="#">Nob Hill</a></li>
                    <li><a href="#">North Beach</a></li>
                    <li><a href="#">Marina</a></li>
                    <li><a href="#">Inner Sunset</a></li>
                    <li><a href="#">Mission</a></li>
                    <li><a href="#">Financial District</a></li>
                  </ul>
                </td>
              </tr>

              <tr class="row" style="margin: 10px; padding: 10px">
                <td class = "background" height="300" width="390" background="/images/places/sd.jpg">
                  <p align="center"><b><i>San Jose</i></b></p>
                  <ul>
                    <li><a href="#">Alum Rock</a></li>
                    <li><a href="#">West San Jose</a></li>
                    <li><a href="#">Downttown San Jose</a></li>
                    <li><a href="#">LBlossom Hill</a></li>
                    <li><a href="#">Willow Glen</a></li>
                    <li><a href="#">North Street</a></li>
                    <li><a href="#">Los Altos Hills</a></li>
                    <li><a href="#">North San Jose</a></li>
                  </ul>
                </td>
                &nbsp;<td height="300" class = "background"  width="390" background="/images/places/ny.jpg">
                  <p align="center" ><b><i>New York</i></b></p>
                  <ul>
                    <li><a href="#">Brooklyn Heights</a></li>
                    <li><a href="#">Carroll Gardens</a></li>
                    <li><a href="#">Clinton Hills</a></li>
                    <li><a href="#">Cobble Hills</a></li>
                    <li><a href="#">Downtown</a></li>
                    <li><a href="#">Park Slope</a></li>
                    <li><a href="#">Prospect Heights</a></li>
                    <li><a href="#">Fort Greene</a></li>
                  </ul>
                </td>
              </tr>
            </table>

          </div>
        </div>
      </div>
    </header>
    <!--End of Header-->
    <!--About-->
    <section class="bg-primary" id="about">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 col-lg-offset-2 text-center">
            <h2 class="section-heading"><%= staticTxt.easy %></h2>
            <hr class="light">
            <p class="text-faded"><%= staticTxt.desc %></p>
            <a href="#" class="btn btn-default btn-xl"><%= staticTxt.seestory %></a>
          </div>
        </div>
      </div>
    </section>
    <!--See how much You can save-->
    <section id="services">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading"><%= staticTxt.howmuchsave %></h2>
            <hr class="primary">
          </div>
        </div>
      </div>
      <div class="container">
        <div class="row">
          <div class="col-md-6 text-center">
            <p><%= staticTxt.sabebuy %></p>
            <svg>
              <circle cx="150" cy="60" r="50" stroke="grey" stroke-width="2" fill="none"/>
              Sorry ur browser doesnot support svg
              <text x="150" y="60"
                    font-family="sans-serif"
                    font-size="20px"
                    text-anchor="middle"
                    fill="red" id="demo">5700
              </text>
            </svg>
          </div>
          <div class="col-md-6 text-center">
            <p><%= staticTxt.savesell %></p>
            <svg>
              <circle cx="150" cy="60" r="50" stroke="grey" stroke-width="2" fill="none"/>
              Sorry ur browser doesnot support svg
              <text x="150" y="60"
                    font-family="sans-serif"
                    font-size="20px"
                    text-anchor="middle"
                    fill="red" id="demo1">6250
              </text>
            </svg>
          </div>
          <div class="row">
            <div class="col-md-12 ">
              <p style="float:left;">$200,000</p>
              <p style="float: right">$1,250,000</p>
              <p style="text-align: center" id="demo2">625000</p>
              <input class = "slider" type="range" id="myRange" value="50" onchange="myFunction()" >
              <p style="text-align:center"><%= staticTxt.adjustslider %></p>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12"></div>
        </div>
      </div>
    </section>
    <!--Section showing carousel for the customer reviews-->
    <div class="img_carousel">
      <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Carousel indicators -->
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
          <li data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
          <li data-target="#myCarousel" data-slide-to="3"></li>
          <li data-target="#myCarousel" data-slide-to="4"></li>
        </ol>
        <!-- Wrapper for carousel items -->
        <div class="carousel-inner">
          <div class="item active">
            <img src="../images/carousel/1.jpg" alt="First Slide">
            <p><%= staticTxt.firstslide %></p>
          </div>
          <div class="item">
            <img src="../images/carousel/2.jpg" alt="Second Slide">
            <p><%= staticTxt.secondslide %></p>
          </div>
          <div class="item">
            <img src= "../images/carousel/3.jpg" alt="Third Slide">
            <p><%= staticTxt.thirdslide %></p>
          </div>
          <div class="item">
            <img src= "../images/carousel/4.jpg" alt="4th Slide">
            <p><%= staticTxt.fourthslide %></p>
          </div>
          <div class="item">
            <img src= "../images/carousel/5.jpg" alt="5th Slide">
            <p><%= staticTxt.fifthslide %></p>
          </div>
        </div>
        <!-- Carousel controls -->
        <a class="carousel-control left" href="#myCarousel" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left"></span>
        </a>
        <a class="carousel-control right" href="#myCarousel" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right"></span>
        </a>
      </div>
    </div>
    <section class="no-padding" id="portfolio">
      <div class="container-fluid">

      </div>
    </section>

    <section class="agent">
      <div class="container">
        <div class="col-lg-8 col-lg-offset-2 text-center">
          <h2 class="section-heading"><%= staticTxt.agentshelp %></h2>
          <hr class="primary">
        </div>
        <div class="col-md-10 col-md-offset-3" align="center">
          <form method = "post" action = "#" class="form-horizontal">

            <!--User ID field-->
            <div class="form-group">
              <label for="name" class="control-label col-xs-2"><%= staticTxt.fullname %></label>
              <div class="col-xs-4">
                <input type="text" class="form-control" id="name" placeholder="Ruchi Shah" required>
              </div>
            </div>
            <!--Email field-->
            <div class="form-group">
              <label for="email" class="control-label col-xs-2"><%= staticTxt.email %></label>
              <div class="col-xs-4">
                <input type="email" class="form-control" id="email" placeholder="ruchi.shah@sjsu.edu" required>
              </div>
            </div>
            <!--Question-->
            <div class="form-group">
              <label for="query" class="control-label col-xs-2"><%= staticTxt.query %></label>
              <div class="col-xs-4">
                <textarea class="form-control" rows="5" id="query" placeholder="YOUR QUERY" required></textarea>
              </div>
            </div>
            <div class="form-group">
              <div class="col-lg-8">
                <input type = "submit" value = "<%= staticTxt.ask %>" style="-webkit-text-fill-color: #222" class="btn btn-primary"></button>
              </div>
            </div>
          </form>
        </div>
      </div>
    </section>

    <section id="contact">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 col-lg-offset-2 text-center">
            <h2 class="section-heading"><%= staticTxt.getintouch %></h2>
            <hr class="primary">
            <p><%= staticTxt.getintouchdescription %></p>
          </div>
          <div class="col-lg-4 col-lg-offset-2 text-center">
            <i class="fa fa-phone fa-3x wow bounceIn"></i>
            <p>123-456-6789</p>
          </div>
          <div class="col-lg-4 text-center">
            <i class="fa fa-envelope-o fa-3x wow bounceIn" data-wow-delay=".1s"></i>
            <p><a href="mailto:your-email@your-domain.com">feedback@homesearch.com</a></p>
          </div>
        </div>
      </div>
    </section>

    <!-- jQuery -->

    <!-- Bootstrap Core JavaScript -->

    <!-- Plugin JavaScript -->
    <!-- Custom Theme JavaScript -->

    <% include jsinclusions.ejs%>
    <script>
    </script>
  </body>
</html>
